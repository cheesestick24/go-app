{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./static/js",
    "rootDir": "./static/ts"
  },
  "include": ["static/ts/**/*"],
  "exclude": ["node_modules"]
}
